Kron:
	'kronos:' 
	jobs += Job
;

Description:
	('-')? 'description:' content=STRING
;

Url:
	'url' location=STRING
;

Job:
	desc = Description
	url = Url
	schedule = Schedule
;

Schedule:
	Every | Ordinal
;


Every:
	'every' n=INT unit=Unit (when = From)?
;

Unit:
	'hours' | 'mins' | 'minutes'
;

From:
	'from' start=STRING 'to' end=STRING
;

Ordinal:
	'milos'
;